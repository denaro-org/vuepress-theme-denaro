    "代码复制"
  ],
  "homepage": "https://github.com/denaro-org/vuepress-theme-denaro/tree/main/packages/@vuepress-denaro/vuepress-plugin-one-click-copy#readme",
  "bugs": {
    "url": "https://github.com/denaro-org/vuepress-theme-denaro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/denaro-org/vuepress-theme-denaro.git"
  },
  "license": "MIT",
  "author": "vxhly <pengchengou@gmail.com>",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "build:watch": "tsc --build -w tsconfig.build.json",
    "clean": "npx rimraf lib *.tsbuildinfo ./**/dist",
    "copy": "cpx \"src/**/*.{d.ts,vue,less,scss,css,html,sass,json,js}\" lib"
  },
  "dependencies": {
    "@vuepress-denaro/core": "1.0.0",
