{
  "extends": "tsconfig-vuepress/base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["DOM", "ESNext", "dom.iterable"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "types": ["@vuepress/client/types"]
  }
}
