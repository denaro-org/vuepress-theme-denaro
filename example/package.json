{
  "name": "example",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "npx rimraf .vuepress/.cache .vuepress/.temp dist",
    "example:build": "vuepress-cli build --clean-cache",
    "example:clean": "rimraf .vuepress/.temp .vuepress/.cache .vuepress/dist",
    "example:dev": "vuepress-cli dev --clean-cache",
    "example:serve": "anywhere -s -h localhost -d .vuepress/dist"
  },
  "dependencies": {
    "@vuepress-denaro/vuepress-plugin-autodoc": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-baidu-autopush": "workspace:*",
