    "@vuepress-denaro/vuepress-plugin-dynamic-title": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-flowchart": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-moefy-canvas": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-one-click-copy": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-permalink-pinyin": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-reward": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-sitemap": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-smooth-scroll": "workspace:*",
    "@vuepress-denaro/vuepress-plugin-vue-preview": "workspace:*",
    "@vuepress/bundler-vite": "2.0.0-beta.61",
    "@vuepress/cli": "2.0.0-beta.61",
    "@vuepress/client": "2.0.0-beta.61",
